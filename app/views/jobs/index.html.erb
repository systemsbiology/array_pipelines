<h1>Listing jobs</h1>

<table>
  <tr>
    <th>Pipeline</th>
    <th>Status</th>
    <th>Start time</th>
    <th>End time</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @jobs.each do |job| %>
  <tr>
    <td><%= job.pipeline %></td>
    <td><%= job.status %></td>
    <td><%= job.start_time %></td>
    <td><%= job.end_time %></td>
    <td><%= link_to 'Show', job %></td>
    <td><%= link_to 'Edit', edit_job_path(job) %></td>
    <td><%= link_to 'Destroy', job, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Job', new_job_path %>
